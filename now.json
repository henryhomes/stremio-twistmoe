{
    "version": 2,
    "builds": [
        { "src": "serverless.js", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/.*", "dest": "/serverless.js" }
    ],
    "env": {
        "REDIS_PASS": "@twist-redis-pass",
        "TWIST_TOKEN": "@twist-token",
        "TWIST_KEY": "@twist-key"
    }
}
